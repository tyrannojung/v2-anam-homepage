<query id="getMemberCountByPhoneCountry" action="select">
    <tables>
        <table name="member" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
		<condition operation="equal" column="phone_country" var="phone_country" notnull="notnull" />
    </conditions>
</query>
