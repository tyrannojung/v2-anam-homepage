<query id="getUserUnsubscribeConfigByUnsubscribeSrl" action="select">
	<tables>
		<table name="ncenterlite_unsubscribe" />
	</tables>

	<columns>
		<column name="*" />
	</columns>

	<conditions>
		<condition operation="equal" column="unsubscribe_srl" var="unsubscribe_srl" filter="number" notnull="notnull" />
	</conditions>
</query>
